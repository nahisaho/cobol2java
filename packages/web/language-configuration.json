{
  "comments": {
    "lineComment": "*"
  },
  "brackets": [
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"" },
    { "open": "'", "close": "'" }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "wordPattern": "[A-Za-z][A-Za-z0-9-]*"
}
